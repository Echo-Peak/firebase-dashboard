{
    "databaseURL": "https://<YOUR PROJECT ID>.firebaseio.com/",
    "admin_service_key": "<location of service key>",
    "defaultNode": "/",
    "persistToDisk": true,
    "watch": false,
    "keyboardSave": true,
    "defaultMode": "json",
    "localDB": "./DB",
    "cacheTracking":true,
    "editor": {
        "showRoot": false,
        "firstRun": true,
        "charLimit": 8000
    },
    "ports": {
        "proxy": 5000,
        "dev": 3000
    },
    "mode": "json"
}
